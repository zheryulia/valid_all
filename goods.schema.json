            "examples": [
                {
                    "width": 5,
                    "height": 10
                }
            ],
            "required": [
                "width",
                "height"
            ],
            "additionalProperties": true,
            "properties": {
                "width": {
                    "$id": "#/properties/package_params/properties/width",
